@model XBC.ViewModel.RoomViewModel

<tr>
    <td>
        @Html.EditorFor(model => model.code, new { htmlAttributes = new { @class = "form-control", Style = "max-width:100%", @readonly = "readonly" } })
    </td>
    <td>
        @Html.EditorFor(model => model.name, new { htmlAttributes = new { @class = "form-control", Style = "max-width:100%", @readonly = "readonly" } })
    </td>
    <td>
        @Html.EditorFor(model => model.capacity, new { htmlAttributes = new { @class = "form-control", Style = "max-width:100%", @readonly="readonly" } })
        @Html.HiddenFor(model => model.any_projector)
        @Html.HiddenFor(model => model.notes)
    </td>
</tr>
